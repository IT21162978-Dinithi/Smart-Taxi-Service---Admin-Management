<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>adminLogin</servlet-name>
		<servlet-class>com.admin.adminLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adminLogin</servlet-name>
		<url-pattern>/admin_login</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>VehicleInsert</servlet-name>
	<servlet-class>com.vehicle.VehicleInsert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VehicleInsert</servlet-name>
		<url-pattern>/vehicle_insert</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>updateAdmin</servlet-name>
	<servlet-class>com.admin.updateAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateAdmin</servlet-name>
		<url-pattern>/admin_update</url-pattern>
	</servlet-mapping>
	
		<servlet>
	<servlet-name>deleteAdmin</servlet-name>
	<servlet-class>com.admin.deleteAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteAdmin</servlet-name>
		<url-pattern>/admin_delete</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>vehicleLogin</servlet-name>
		<servlet-class>com.vehicle.VehicleRegLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>vehicleLogin</servlet-name>
		<url-pattern>/vehicle_login</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>deleteVehiclePre</servlet-name>
	<servlet-class>com.vehicle.deleteVehicleServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteVehiclePre</servlet-name>
		<url-pattern>/vehicle_delete</url-pattern>
	</servlet-mapping>
	
	
	
	
	
    
</web-app>